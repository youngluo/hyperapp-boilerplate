{
  "name": "hyperapp-boilerplate",
  "version": "1.0.0",
  "description": "A boilerplate project that shows how to create a hyperapp application quickly",
  "scripts": {
    "start": "cross-env NODE_ENV=development npx webpack-dev-server --open --progress --colors --mode=development",
